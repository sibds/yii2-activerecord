{
    "name": "sibds/yii2-activerecord",
    "description": "Expanding ActiveRecord framework Yii2",
    "keywords": ["components", "active record", "yii2"],
    "homepage": "https://github.com/sibds/yii2-activerecord",
    "type": "yii2-components",
    "license": "MIT",
    "authors": [
        {
            "name": "Vadim Mazur",
            "email": "mazurva@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/sibds/yii2-activerecord/issues",
        "source": "https://github.com/sibds/yii2-activerecord"
    },
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "^2.0.38",
        "creocoder/yii2-nested-sets": "0.9.*",
        "sibds/yii2-trash-behavior": "^0.4"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "~2.0"
    },
    "config": {
        "secure-http": false,
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "autoload": {
        "psr-4": {
            "sibds\\components\\": "",
            "sibds\\behaviors\\": ""
        }
    },
    "repositories": [        
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
